<div id="<%= dom_id client %>">
  <p>
    <strong>Photo:</strong>
    <%= client.photo %>
  </p>

  <p>
    <strong>First name:</strong>
    <%= client.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= client.last_name %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= client.phone %>
  </p>

  <p>
    <strong>Bio:</strong>
    <%= client.bio %>
  </p>

  <p>
    <strong>User:</strong>
    <%= client.user_id %>
  </p>

  <p>
    <strong>Account:</strong>
    <%= client.account_id %>
  </p>

</div>
